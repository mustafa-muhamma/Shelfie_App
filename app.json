{
  "expo": {
    "scheme": "shelfieapp",
    "name": "Shelfie_App",
    "slug": "Shelfie_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_light.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/white logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        // "foregroundImage": "./assets/images/white logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}